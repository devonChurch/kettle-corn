{
  "name": "kettle-corn",
  "version": "1.0.0",
  "description": "React static site generator using Gatsby",
  "main": "index.js",
  "scripts": {
    "develop": "cd ./gatsby-site && gatsby develop",
    "build": "cd ./gatsby-site && gatsby build -- --prefix-paths",
    "gh-pages": "gh-pages -d ./gatsby-site/public",
    "deploy": "npm run build && npm run copy && npm run gh-pages",
    "copy": "npm run copy:cname && npm run copy:favicon && npm run copy:icon && npm run copy:webmanifest && npm run copy:robots",
    "copy:cname": "cp ./gatsby-site/src/CNAME ./gatsby-site/public",
    "copy:favicon": "cp ./gatsby-site/src/favicon.ico ./gatsby-site/public",
    "copy:icon": "cp ./gatsby-site/src/icon.png ./gatsby-site/public",
    "copy:webmanifest": "cp ./gatsby-site/src/site.webmanifest ./gatsby-site/public",
    "copy:robots": "cp ./gatsby-site/src/robots.txt ./gatsby-site/public",
    "test": "echo \"Error: no test specified\" && exit 1",
    "gatsby": "gatsby",
    "commit": "git-cz",
    "commitizen": "commitizen"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devonChurch/kettle-corn.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devonChurch/kettle-corn/issues"
  },
  "homepage": "https://github.com/devonChurch/kettle-corn#readme",
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "gatsby-cli": "^1.1.7",
    "gh-pages": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "gatsby-plugin-styled-components": "^1.0.5",
    "gatsby-plugin-typography": "^1.7.10",
    "lodash.capitalize": "^4.2.1",
    "lodash.random": "^3.2.0",
    "lorem-ipsum": "^1.0.4",
    "markdown-to-jsx": "^5.4.2",
    "polished": "^1.7.0"
  }
}
